<h1>Posts</h1>

<table>
	<thead>
		<tr>
			<th>Title</th>
		</tr>
	</thead>
	<tbody>
		<% @posts.each do |post| %>
			<tr>
				<td><%= post.title %></td>
				<td><%= link_to 'Show details', post_path(post) %></td>
			</tr>
		<% end %>
		<tr>
			<td><%= link_to 'Create a new post', new_post_path %></td>
		</tr>
	</tbody>
</table> 